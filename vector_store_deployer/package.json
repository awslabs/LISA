  "license": "Apache-2.0",
  "dependencies": {
    "@aws-sdk/client-iam": "^3.906.0",
    "aws-cdk": "^2.1029.4",
    "aws-cdk-lib": "^2.219.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "esbuild": "^0.25.1",
    "@cdklabs/cdk-enterprise-iac": "^0.1.0",
    "@types/aws-lambda": "8.10.155",
    "@types/node": "^24.7.0"
  }
}
