{
  "name": "@awslabs/lisa",
  "version": "3.5.1",
  "description": "A scalable infrastructure-as-code solution for self-hosting and orchestrating LLM inference with RAG capabilities, providing low-latency access to generative AI and embedding models across multiple providers.",
  "homepage": "https://awslabs.github.io/LISA/",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/awslabs/lisa.git"
  },
  "bugs": {
    "url": "https://github.com/awslabs/lisa/issues"
  },
  "author": {
    "name": "Amazon Web Services",
    "url": "https://aws.amazon.com"
  },
  "keywords": [
    "aws",
    "bedrock",
    "chatbot",
    "generative-ai",
    "langchain",
    "litellm",
    "llm",
    "large-language-model",
    "openai",
    "rag",
    "retrieval-augmented-generation"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "workspaces": [
    "lib/user-interface/react",
    "lib/docs",
    "ecs_model_deployer",
    "vector_store_deployer"
  ],
  "scripts": {
    "build": "tsc && npm run build --workspaces",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "prepare": "husky install",
    "migrate-properties": "node ./scripts/migrate-properties.mjs",
    "generateSchemaDocs": "npx zod2md -c ./lib/zod2md.config.ts"
  },
  "devDependencies": {
    "@aws-cdk/aws-lambda-python-alpha": "2.125.0-alpha.0",
    "@aws-sdk/client-iam": "^3.738.0",
    "@cdklabs/cdk-enterprise-iac": "^0.0.512",
    "@stylistic/eslint-plugin": "^2.13.0",
    "@types/jest": "^29.5.14",
    "@types/js-yaml": "^4.0.9",
    "@types/lodash": "^4.17.15",
    "@types/node": "20.5.3",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "aws-cdk": "^2.177.0",
    "depcheck": "^1.4.7",
    "eslint": "^8.57.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.18",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.4.3",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.1.6",
    "zod2md": "^0.1.4"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.177.0",
    "aws-sdk": "^2.1692.0",
    "cdk-nag": "^2.35.5",
    "constructs": "^10.4.2",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "source-map-support": "^0.5.21",
    "util": "^0.12.5",
    "zod": "^3.24.1"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
